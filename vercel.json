{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "resume-match-server/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "resume-match-server/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "resume-match/$1"
    }
  ]
}

